var myWindow = Ti.UI.createWindow({
	backgroundImage: "background.png"
});

var myTitle = Ti.UI.createLabel({
	text: "About Me",
	font: {fontStyle: "Roboto", fontSize: 36},
	align: "center",
	top: 40
});

var startButton = Ti.UI.createView({
	bottom: 50,
	left: 40,
	right: 40,
	color: "#fff",
	text: "Start"
});


myWindow.add(myTitle, startButton);
myWindow.open();
