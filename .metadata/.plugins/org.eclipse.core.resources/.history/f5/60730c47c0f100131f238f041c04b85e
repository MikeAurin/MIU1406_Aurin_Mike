var myWindow = Ti.UI.createWindow({
	backgroundImage: "background.png"
});

var navView = Ti.UI.iOS.createNavigationWindow([
]);

var myTitle = Ti.UI.createLabel({
	text: "About Me",
	font: {fontStyle: "Roboto", fontSize: 36},
	align: "center",
	top: 40
});

//require(JSON.js);


myWindow.add(myTitle, navView);
myWindow.open();
